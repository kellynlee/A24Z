  /* margin-top:-20rpx; */
}

.detail {
  font-size: 30rpx;
}

span.detail {
  color: #1296db;
  font-size: 30rpx;
  margin-right: 20rpx; 
}

span.is-read{
  color: #62b900;
  font-size: 40rpx;
}
span.has-read {
  opacity: 0;
  font-size: 40rpx;
}

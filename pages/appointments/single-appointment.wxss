}

.flex-img {
  display: flex;
  align-items: center;
}

.subject {
  font-size: 30rpx;
  padding: 0 0 5rpx 20rpx;
  color:#1296db
}

.owner {
  font-size: 24rpx;
  padding: 0 0 0 20rpx
}

.time {
  font-size: 20rpx;
  padding: 0 0 0 20rpx;
  color: #8a8a8a  
}

.drag-list {
  height: 180rpx;
  background-color: #fff;
